
public class Funciones_sin_datos_de_entrada_ni_salida {

	public static void borrarPantalla()
	{
		for (int i = 0; i<26; i++)
			{
				System.out.println(1);
			}
	}
	
	public static void dibujarCuadrado3x3()
	{
		for (int i=0; i<3; i++)
		{
			System.out.println("* * *");
		}
	}
	
	public static void main(String[] args) {
		
		borrarPantalla();
		dibujarCuadrado3x3();
	}

}
