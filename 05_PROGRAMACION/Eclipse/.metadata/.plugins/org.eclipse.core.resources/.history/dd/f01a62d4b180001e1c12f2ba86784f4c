
public class Funciones_con_datos_de_entrada {
	
	public static void dibujarRectangulo(int ancho, int alto)
	{
		for (int i=alto; i>0; i--) 
		{
			for (int i=ancho; i>0; i--)
			{
				System.out.print("*");
			}
			System.out.println();
		}
	}
	
	public static void main(String[] args)
    {
        dibujarRectangulo(5,3);
    }
}
