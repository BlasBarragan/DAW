import java.util.Scanner;
public class Ejercicio04 {

	public static void main(String[] args) {

		int par;
		int impar;
		
		Scanner sc = new Scanner(System.in);

		System.out.println("Dime un numero: ");
		sc.nextLine();
		int numero = sc.nextInt();
		System.out.println(numero % 10);
	}

}
