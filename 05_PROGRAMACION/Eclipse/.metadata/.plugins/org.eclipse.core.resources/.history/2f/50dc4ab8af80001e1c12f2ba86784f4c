public class Ejercicio42
{
    static void dibujarCuadrado3x3()
    {
        for (int i = 0; i < 3; i++)
        {
            for (int j = 0; j < 3; j++)
                System.out.print("#");
            System.out.println();
        }
    }

    public static void main(String[] args)
    {
        dibujarCuadrado3x3();
    }
}
